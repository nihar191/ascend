{
    "name": "ascend-backend",
    "version": "1.0.0",
    "description": "Real-time competitive programming platform backend",
    "main": "server.js",
    "type": "module",
    "scripts": {
        "start": "node server.js",
        "dev": "nodemon server.js",
        "migrate": "node -e \"import('./config/database.js').then(db => db.runMigrations())\"",
        "seed": "psql -U postgres -d ascend_db -f seeds/seed.sql",
        "seed:ai": "node scripts/SeedAIProblems.js",
        "promote-admin": "node scripts/promoteToAdmin.js",
        "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js"
    },
    "dependencies": {
        "@google/generative-ai": "^0.2.1",
        "axios": "^1.12.2",
        "bcryptjs": "^2.4.3",
        "cors": "^2.8.5",
        "dotenv": "^16.3.1",
        "express": "^4.18.2",
        "express-rate-limit": "^7.1.5",
        "express-validator": "^7.0.1",
        "helmet": "^7.1.0",
        "jsonwebtoken": "^9.0.2",
        "pg": "^8.11.3",
        "socket.io": "^4.7.2"
    },
    "devDependencies": {
        "jest": "^29.7.0",
        "nodemon": "^3.0.2"
    }
}
